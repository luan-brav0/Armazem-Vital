{
  "expo": {
    "name": "lul-yarn-expo-tailwind",
    "slug": "lul-yarn-expo-tailwind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./source/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./source/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./source/assets/favicon.png"
    }
  }
}
